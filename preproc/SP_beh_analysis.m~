function SP_beh_analysis(Sbj_Metadata)
% In order to create behavioral data results (quicker)

% Load response table
real2_response_table = readtable(fullfile(Sbj_Metadata.behavioral_root, curr_block, [curr_block '_response_table.xlsx']));

% Read conditions:
all_cond_codes = sort(unique(real2_response_table.Condition_Code));
for c=1:length(all_cond_codes)
    all_cond_names{c} = real2_response_table.Condition_Name{real2_response_table.Condition_Code==all_cond_codes(c)};
end

% collect correct response table for each condition
for c=1:length(all_cond_codes)
    cond_corrects_tables{c} = ismember(real2_response_table{real2_response_table.Condition_Code==all_cond_codes(c),4:8},'1');
    cond_corrects{c} = sum(sum(cond_corrects_tables{c}));
    subj_corr{c} = sum(ismember(real2_response_table{real2_response_table.Condition_Code==all_cond_codes(c),4},'1'));
    verb_corr{c} = sum(ismember(real2_response_table{real2_response_table.Condition_Code==all_cond_codes(c),5},'1'));
    num_corr{c} = sum(ismember(real2_response_table{real2_response_table.Condition_Code==all_cond_codes(c),6},'1'));
    adj_corr{c} = sum(ismember(real2_response_table{real2_response_table.Condition_Code==all_cond_codes(c),7},'1'));
    noun_corr{c} = sum(ismember(real2_response_table{real2_response_table.Condition_Code==all_cond_codes(c),8},'1'));
end

% Conditions in bar graph
figure('Position',[0 0 1000 1000])
bar([cond_corrects{:}])
title('Correct responses for each condition (/100)')
xticklabels([all_cond_names(:)])

% Correct responses for each word
subj_corr = ismember(real2_response_table{:,4},'1');
verb_corr = ismember(real2_response_table{:,5},'1');
num_corr = ismember(real2_response_table{:,6},'1');
adj_corr = ismember(real2_response_table{:,7},'1');
noun_corr = ismember(real2_response_table{:,8},'1');

figure('Position',[0 0 1000 1000])
bar([subj_corr{:};...
    verb_corr{:};...
    num_corr{:};...
    adj_corr{:};...
    noun_corr{:}])

ylim([0 20])
set(gca,'xticklabels',{'Subject','Verb','Number','Adjective','Noun'},'FontSize',15)




















end